{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ProductInformationPage",
  "description": "Schema for Home Depot Product Information Page (PIP) data",
  "type": "object",
  "required": ["productId", "identifiers", "brand", "title", "pricing", "media", "specifications"],
  "properties": {
    "productId": {
      "type": "string",
      "description": "Home Depot Internet Number"
    },
    "identifiers": {
      "type": "object",
      "required": ["internetNumber", "modelNumber", "storeSkuNumber"],
      "properties": {
        "internetNumber": { "type": "string" },
        "modelNumber": { "type": "string" },
        "storeSkuNumber": { "type": "string" },
        "storeSoSkuNumber": { "type": "string" },
        "upc": { "type": "string" },
        "manufacturerPartNumber": { "type": "string" }
      }
    },
    "brand": {
      "type": "object",
      "required": ["name"],
      "properties": {
        "name": { "type": "string" },
        "logoUrl": { "type": "string" },
        "brandPageUrl": { "type": "string" },
        "series": { "type": "string" }
      }
    },
    "title": { "type": "string" },
    "shortDescription": { "type": "string" },
    "longDescription": { "type": "string" },
    "breadcrumbs": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": { "type": "string" },
          "url": { "type": "string" }
        }
      }
    },
    "pricing": {
      "type": "object",
      "required": ["currentPrice"],
      "properties": {
        "currentPrice": { "type": "number" },
        "originalPrice": { "type": "number" },
        "savings": { "type": "number" },
        "savingsPercent": { "type": "integer" },
        "unitOfMeasure": { "type": "string" },
        "pricePerUnit": { "type": "string" },
        "isOnSale": { "type": "boolean" },
        "seeInCart": { "type": "boolean" },
        "bulkPricing": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "minQuantity": { "type": "integer" },
              "price": { "type": "number" }
            }
          }
        },
        "financing": {
          "type": "object",
          "properties": {
            "monthlyPayment": { "type": "number" },
            "term": { "type": "integer" },
            "apr": { "type": "number" }
          }
        },
        "proPrice": { "type": "number" }
      }
    },
    "rating": {
      "type": "object",
      "properties": {
        "average": { "type": "number" },
        "count": { "type": "integer" },
        "recommendationPercent": { "type": "integer" },
        "distribution": {
          "type": "object",
          "properties": {
            "5star": { "type": "integer" },
            "4star": { "type": "integer" },
            "3star": { "type": "integer" },
            "2star": { "type": "integer" },
            "1star": { "type": "integer" }
          }
        }
      }
    },
    "media": {
      "type": "object",
      "properties": {
        "primaryImage": { "type": "string" },
        "images": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "url": { "type": "string" },
              "altText": { "type": "string" },
              "type": {
                "type": "string",
                "enum": ["primary", "lifestyle", "detail", "swatch", "diagram"]
              }
            }
          }
        },
        "videos": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "videoId": { "type": "string" },
              "title": { "type": "string" },
              "thumbnailUrl": { "type": "string" },
              "videoUrl": { "type": "string" },
              "duration": { "type": "string" }
            }
          }
        },
        "threeSixtyView": { "type": "string" },
        "augmentedReality": { "type": "boolean" }
      }
    },
    "variants": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "variantId": { "type": "string" },
          "variantType": { "type": "string" },
          "variantValue": { "type": "string" },
          "price": { "type": "number" },
          "imageUrl": { "type": "string" },
          "swatchUrl": { "type": "string" },
          "inStock": { "type": "boolean" },
          "isSelected": { "type": "boolean" }
        }
      }
    },
    "keyFeatures": {
      "type": "array",
      "items": { "type": "string" }
    },
    "highlights": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "icon": { "type": "string" },
          "label": { "type": "string" },
          "value": { "type": "string" }
        }
      }
    },
    "specifications": {
      "type": "object",
      "properties": {
        "dimensions": {
          "type": "object",
          "properties": {
            "productWidth": { "type": "string" },
            "productHeight": { "type": "string" },
            "productDepth": { "type": "string" },
            "productWeight": { "type": "string" },
            "packageWidth": { "type": "string" },
            "packageHeight": { "type": "string" },
            "packageDepth": { "type": "string" },
            "packageWeight": { "type": "string" }
          }
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "groupName": { "type": "string" },
              "attributes": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "name": { "type": "string" },
                    "value": { "type": "string" }
                  }
                }
              }
            }
          }
        }
      }
    },
    "badges": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "enum": ["topSeller", "newArrival", "exclusive", "energyStar", "specialBuy", "limitedStock", "proPreferred", "bestSeller"]
          },
          "label": { "type": "string" },
          "iconUrl": { "type": "string" }
        }
      }
    },
    "certifications": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "logoUrl": { "type": "string" },
          "description": { "type": "string" }
        }
      }
    },
    "warranty": {
      "type": "object",
      "properties": {
        "manufacturerWarranty": { "type": "string" },
        "extendedWarrantyAvailable": { "type": "boolean" },
        "warrantyDetails": { "type": "string" }
      }
    },
    "protectionPlan": {
      "type": "object",
      "properties": {
        "available": { "type": "boolean" },
        "options": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "term": { "type": "integer" },
              "price": { "type": "number" },
              "coverage": { "type": "string" }
            }
          }
        }
      }
    },
    "availability": {
      "type": "object",
      "properties": {
        "inStorePickup": {
          "type": "object",
          "properties": {
            "available": { "type": "boolean" },
            "quantity": { "type": "integer" },
            "storeName": { "type": "string" },
            "storeId": { "type": "string" }
          }
        },
        "delivery": {
          "type": "object",
          "properties": {
            "available": { "type": "boolean" },
            "estimatedDate": { "type": "string" },
            "freeDelivery": { "type": "boolean" },
            "deliveryCharge": { "type": "number" }
          }
        },
        "shipToHome": {
          "type": "object",
          "properties": {
            "available": { "type": "boolean" },
            "estimatedDate": { "type": "string" },
            "shippingCharge": { "type": "number" },
            "freeShipping": { "type": "boolean" }
          }
        }
      }
    },
    "services": {
      "type": "object",
      "properties": {
        "installation": {
          "type": "object",
          "properties": {
            "available": { "type": "boolean" },
            "price": { "type": "number" },
            "description": { "type": "string" }
          }
        },
        "haulAway": {
          "type": "object",
          "properties": {
            "available": { "type": "boolean" },
            "price": { "type": "number" }
          }
        },
        "assembly": {
          "type": "object",
          "properties": {
            "available": { "type": "boolean" },
            "price": { "type": "number" }
          }
        }
      }
    },
    "requirements": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "type": { "type": "string" },
          "description": { "type": "string" }
        }
      }
    },
    "includedItems": {
      "type": "array",
      "items": { "type": "string" }
    },
    "relatedProducts": {
      "type": "object",
      "properties": {
        "accessories": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "productId": { "type": "string" },
              "title": { "type": "string" },
              "price": { "type": "number" },
              "imageUrl": { "type": "string" }
            }
          }
        },
        "similar": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "productId": { "type": "string" },
              "title": { "type": "string" },
              "price": { "type": "number" },
              "imageUrl": { "type": "string" }
            }
          }
        },
        "frequentlyBoughtTogether": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "productId": { "type": "string" },
              "title": { "type": "string" },
              "price": { "type": "number" },
              "imageUrl": { "type": "string" }
            }
          }
        }
      }
    },
    "questionsAndAnswers": {
      "type": "object",
      "properties": {
        "totalQuestions": { "type": "integer" },
        "featured": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "questionId": { "type": "string" },
              "question": { "type": "string" },
              "answer": { "type": "string" },
              "askedDate": { "type": "string" },
              "answeredDate": { "type": "string" },
              "helpful": { "type": "integer" }
            }
          }
        }
      }
    },
    "returnPolicy": {
      "type": "object",
      "properties": {
        "returnWindow": { "type": "integer" },
        "returnWindowUnit": { "type": "string" },
        "specialPolicy": { "type": "string" }
      }
    },
    "limitPerOrder": { "type": "integer" },
    "productUrl": { "type": "string" },
    "canonicalUrl": { "type": "string" },
    "seoMetadata": {
      "type": "object",
      "properties": {
        "title": { "type": "string" },
        "description": { "type": "string" },
        "keywords": {
          "type": "array",
          "items": { "type": "string" }
        }
      }
    }
  }
}
